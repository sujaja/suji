s2=int(input('enter marks of student 2:'))
s3=int(input('enter marks of student 3:'))
print('sno:',sno)
print('name:',name)
print('group:',group)
print('student 1:',s1)
print('student 2:',s2)
print('student 3:',s3)
print('passed')if(s1>=30 and s2>30 and s3>30)else print('failed')


